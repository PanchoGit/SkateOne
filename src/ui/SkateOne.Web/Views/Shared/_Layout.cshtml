<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <script src="~/dist/lib/jquery-1.8.2.js"></script>
        <script src="~/dist/lib/plugins/CSSPlugin.min.js"></script>
        <script src="~/dist/lib/easing/EasePack.min.js"></script>
        <script src="~/dist/lib/TweenLite.min.js"></script>
        <script src="~/dist/lib/GridNess.js"></script>

        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href="~/dist/css/App.css" />
        <link rel="stylesheet" type="text/css" href="~/dist/css/GridNess.css" />
    </head>

<body style="overflow:hidden">
    <div id="waitDiv" class="statusBar wait statusText" align="center"><img src="~/dist/i/indicator.gif" width="16" height="16">Loading...</div>

    <h1 class="mainTitle">@ViewBag.Title</h1>

    @RenderBody()

</body>
</html>
